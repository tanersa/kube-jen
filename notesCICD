sudo docker container run --name jenkins-docker --detach --restart unless-stopped --privileged --network jenkins --network-alias docker --env DOCKER_TLS_CERTDIR="" --volume jenkins-docker-certs:/certs/client --volume jenkins-data:/var/jenkins_home docker:dind


docker container run --name jenkins-blueocean \
  --detach --restart unless-stopped \
  --network jenkins \
  --env DOCKER_HOST="tcp://docker:2375" \
  --env DOCKER_CERT_PATH="" \
  --env DOCKER_TLS_VERIFY="" \
  --volume jenkins-docker-certs:/certs/client:ro \
  --volume jenkins-data:/var/jenkins_home \
  --publish 8080:8080 --publish 50000:50000 \
  jenkinsci/blueocean
